<h1>New User</h1>

<%= form_for(@user) do |f| %>
  <div class="field">
    <%= f.label :uid %>
    <%= f.text_field :uid %>
  </div>

  <div class="field">
    <%= f.label :bus %>
    <%= f.text_field :bus%>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>


<%= link_to 'Back', users_path %>
